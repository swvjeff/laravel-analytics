{
    "name": "swvjeff/laravel-analytics",
    "description": "A simple package that allows you to define multiple Google Analytics tracking IDs for your website",
    "authors": [
        {
            "name": "Jeff Smith",
            "email": "jeff@selectwv.com",
            "homepage": "https://www.selectwv.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Swvjeff\\Analytics\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swvjeff\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "Swvjeff\\Analytics\\AnalyticsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
